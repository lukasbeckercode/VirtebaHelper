<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lukasbeckercode.virtebahelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VirtEBAHelper"
        >
        <activity android:name=".StartUpActivity" >
        <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
         </intent-filter>
        </activity>

        <activity android:name=".TypeAndCodeActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".AgeCalcActivity" />
        <activity
            android:name=".CategoryViewActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".SearcherActivity" />
    </application>

</manifest>